{
  "name": "quiz-mtc",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "vercel-build": "react-scripts build",
    "dev:legacy": "npm run api & npm start",
    "dev": "concurrently \"npm:api:test\" \"npm:start\"",
    "dev:vercel": "vercel dev --listen 3001",
    "dev:express": "concurrently \"npm:api:test\" \"npm:start\"",
    "api": "node server.js",
    "api:test": "cross-env NODE_ENV=development node server.js",
    "api:prod": "cross-env NODE_ENV=production node server.js",
    "send:test": "cross-env NODE_ENV=development node diagnostico-automacao-lotes.js",
    "send:prod": "cross-env NODE_ENV=production node diagnostico-automacao-lotes.js",
    "verify:test": "cross-env NODE_ENV=development node verificar-leads.js",
    "verify:prod": "cross-env NODE_ENV=production node verificar-leads.js",
    "monitor:prod": "cross-env NODE_ENV=production node monitorar-envio.js",
    "gatilho:test": "cross-env NODE_ENV=development node diagnostico-automacao-individual.js",
    "gatilho:prod": "cross-env NODE_ENV=production node diagnostico-automacao-individual.js",
    "diag:lotes:test": "cross-env NODE_ENV=development node diagnostico-automacao-lotes.js",
    "diag:lotes:prod": "cross-env NODE_ENV=production node diagnostico-automacao-lotes.js",
    "diag:individual:test": "cross-env NODE_ENV=development node diagnostico-automacao-individual.js",
    "diag:individual:prod": "cross-env NODE_ENV=production node diagnostico-automacao-individual.js",
    "template:test": "cross-env NODE_ENV=development node enviar-template-com-gatilho.js",
    "template:prod": "cross-env NODE_ENV=production node enviar-template-com-gatilho.js",
    "desafio:test": "cross-env NODE_ENV=development node desafio-envio-lotes.js",
    "desafio:prod": "cross-env NODE_ENV=production node desafio-envio-lotes.js",
    "desafio:individual:test": "cross-env NODE_ENV=development node desafio-envio-individual.js",
    "desafio:individual:prod": "cross-env NODE_ENV=production node desafio-envio-individual.js",
    "tags:backfill:test": "cross-env NODE_ENV=development node scripts/backfill-status-tags.js",
    "tags:backfill:prod": "cross-env NODE_ENV=production node scripts/backfill-status-tags.js",
    "dupes:report": "cross-env NODE_ENV=production node relatorios/relatorio-duplicados-telefones.js",
    "dupes:fix:test": "cross-env NODE_ENV=development DRY_RUN=1 node relatorios/resolver-duplicados-telefones.js",
    "dupes:fix:prod": "cross-env NODE_ENV=production DRY_RUN=1 node relatorios/resolver-duplicados-telefones.js",
    "normalize:test": "cross-env NODE_ENV=development DRY_RUN=1 node normalizar-telefones.js",
    "normalize:prod": "cross-env NODE_ENV=production DRY_RUN=1 node normalizar-telefones.js",
    "check-sync": "node check-sync.js",
    "migrate:phones:dry": "node scripts/migrate-phones-to-e164.js --dry-run",
    "migrate:phones:apply": "node scripts/migrate-phones-to-e164.js --apply",
    "duplicados:dry": "node scripts/remover-duplicados.js --dry-run",
    "duplicados:apply": "node scripts/remover-duplicados.js --apply",
    "remover-nao-migrados:dry": "node scripts/remover-nao-migrados.js --dry-run",
    "remover-nao-migrados:apply": "node scripts/remover-nao-migrados.js --apply",
    "migrar-duplicados:dry": "node scripts/migrar-e-remover-duplicados.js --dry-run",
    "migrar-duplicados:apply": "node scripts/migrar-e-remover-duplicados.js --apply",
    "audio:test": "cross-env NODE_ENV=development node mensagem-voz-personalizada.js",
    "audio:prod": "cross-env NODE_ENV=production node mensagem-voz-personalizada.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "libphonenumber-js": "^1.12.25",
    "lucide-react": "^0.263.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "recharts": "^3.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vercel": "^48.7.0"
  },
  "proxy": "http://localhost:3001"
}
