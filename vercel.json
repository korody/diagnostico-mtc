{
  "redirects": [
    { "source": "/enviar-diagnosticos", "destination": "/api/enviar-diagnosticos", "permanent": false },
    { "source": "/admin/enviar-diagnosticos", "destination": "/api/enviar-diagnosticos", "permanent": false }
  ],
  "rewrites": [
    { "source": "/dashboard", "destination": "/api/dashboard-page" },
    { "source": "/webhook/unnichat/ver-resultados", "destination": "/api/webhook/unnichat/ver-resultados" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "crons": [
    { "path": "/api/cron/daily-summary", "schedule": "0 21 * * *" }
  ]
}